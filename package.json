{
  "name": "mn-discord-bot",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc",
    "start": "node dist/src/server.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "vercel-build": "node scripts/registerCommands.js && mkdir -p public && echo 'placeholder' > public/.vercelkeep",
    "build:public": "mkdir -p public",
    "register:commands": "node scripts/registerCommands.js",
    "tests": "jest",
    "tests:watch": "jest --watch",
    "server-search": "node scripts/serverSearch.js"
  },
  "dependencies": {
    "discord.js": "^14.13.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "@supabase/supabase-js": "^2.0.0",
    "pg": "^8.11.3",
    "skia-canvas": "^2.0.2",
    "discord-interactions": "^2.3.0",
    "typescript": "^5.2.2",
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.17",
    "p-queue": "^8.0.1"
  },
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0",
    "tsx": "^3.12.7",
    "@vercel/node": "^1.15.0",
    "@types/pg": "^8.6.6",
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.17",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.3",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12"
  }
}
